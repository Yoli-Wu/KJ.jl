/* Emacs: Alt-x maxima, Alt-x maxima-mode */

kill(all) $

K: Xm - X*ft*FT - bXt $
L: Ym - A*Z - B*X - bYt $
M: Zm - Z*exp(c) - bZt $

SS: K^2 + L^2 + M^2 $

dSSdX: diff(SS,X) $
dSSdZ: diff(SS,Z) $

sol: solve([dSSdX,dSSdZ],[X,Z]) $

/*
K: Xm - exp(lX)*ft*fT - bXt $
L: Ym - A*exp(lZ) - B*exp(lX) - bYt $
M: Zm - exp(lZ+c) - bZt $

SS: K^2 + L^2 + M^2 $

dSSdlX: diff(SS,lX) $
dSSdlZX: diff(SS,lZX) $

sol: solve([dSSdlX,dSSdlZX],[lX,lZX]) $
*/
/* Emacs: Alt-x maxima, Alt-x maxima-mode */

kill(all) $

K: Xm - X - bXt $
L: Ym - A*Z*exp(c) - B*X*ft*FT - bYt $
M: Zm - Z - bZt $

SS: K^2 + L^2 + M^2 $

dSSdX: diff(SS,X) $
dSSdZ: diff(SS,Z) $

sol: solve([dSSdX,dSSdZ],[X,Z]) $

/*
K: Xm - exp(lX) - bXt $
L: Ym - A*exp(lZ)*exp(c) - B*exp(lX)*ft*fT - bYt $
M: Zm - exp(lZ) - bZt $

SS: K^2 + L^2 + M^2 $

dSSdlX: diff(SS,lX) $
dSSdlZX: diff(SS,lZX) $

sol: solve([dSSdlX,dSSdlZX],[lX,lZX]) $
*/